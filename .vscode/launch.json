{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run on Kubernetes - cloudbuild",
        "type": "cloudcode.kubernetes",
        "request": "launch",
        "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
        "profile": "cloudbuild",
        "watch": true,
        "cleanUp": true,
        "portForward": true,
        "imageRegistry": "gcr.io/household-iot-277519"
      },
      {
        "type": "cloudcode",
        "request": "attach",
        "name": "Debug on Kubernetes (frontend)",
        "language": "Python",
        "debugPort": 3000,
        "localRoot": "${workspaceFolder}/frontend",
        "remoteRoot": "/app",
        "podSelector": {
          "app": "python-garage",
          "tier": "frontend"
        }
      },
      {
        "type": "cloudcode",
        "request": "attach",
        "name": "Debug on Kubernetes (backend)",
        "language": "Python",
        "debugPort": 3000,
        "localRoot": "${workspaceFolder}/backend",
        "remoteRoot": "/app",
        "podSelector": {
          "app": "python-garage",
          "tier": "backend"
        }
      }
    ]
  }
  